{% extends 'base.html.twig' %}

{% block title %}Formquestions index{% endblock %}

{% block body %}
    <h1>Formquestions index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for formquestion in formquestions %}
            <tr>
                <td>{{ formquestion.id }}</td>
                <td>
                    <a href="{{ path('app_formquestions_show', {'id': formquestion.id}) }}">show</a>
                    <a href="{{ path('app_formquestions_edit', {'id': formquestion.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="2">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_formquestions_new') }}">Create new</a>
{% endblock %}
